<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ResultsController">
   <children>
      <Button fx:id="btn_chooseDoc" layoutX="419.0" layoutY="310.0" mnemonicParsing="false" onAction="#takeDocNameFromGUI" text="Show Details" />
      <BorderPane fx:id="bp_pane" layoutX="74.0" layoutY="19.0" prefHeight="284.0" prefWidth="425.0" />
      <Label layoutX="74.0" layoutY="314.0" text="Choose Document and press the button to show more details:" />
      <Button fx:id="btn_save" layoutX="420.0" layoutY="350.0" mnemonicParsing="false" onAction="#WriteResultsToFile" text="Save Results" />
      <TextField fx:id="tf_savingPath" layoutX="188.0" layoutY="350.0" />
      <Button fx:id="btn_browse" layoutX="348.0" layoutY="350.0" mnemonicParsing="false" onAction="#browse" text="browse" />
      <Text layoutX="74.0" layoutY="367.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Path to save Results:" />
   </children>
</AnchorPane>
